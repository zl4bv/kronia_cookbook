[global_config]
  enabled_plugins = LaunchpadCodeURLHandler, APTURLHandler, LaunchpadBugURLHandler
  title_transmit_fg_color = "#5fffaf"
  title_inactive_fg_color = "#d1d1d1"
  title_receive_bg_color = "#1c1c1c"
  title_transmit_bg_color = "#1c1c1c"
  title_receive_fg_color = "#5fffaf"
  tab_position = bottom
  title_inactive_bg_color = "#444444"
[keybindings]
[profiles]
  [[default]]
    scrollbar_position = hidden
    palette = "#2d2d2d:#f2777a:#99cc99:#ffcc66:#6699cc:#cc99cc:#66cccc:#d3d0c8:#747369:#f2777a:#99cc99:#ffcc66:#6699cc:#cc99cc:#66cccc:#f2f0ec"
    background_image = None
    background_darkness = 0.92
    background_type = transparent
    use_system_font = False
    split_to_group = True
    foreground_color = "#d3d0c8"
    icon_bell = False
    cursor_color = "#d3d0c8"
    font = Source Code Pro for Powerline Medium 10
    background_color = "#2d2d2d"
    show_titlebar = false
[layouts]
  [[default]]
    [[[child1]]]
      type = Terminal
      parent = window0
      profile = default
    [[[window0]]]
      type = Window
      parent = ""
      size = 900, 600
[plugins]
